{
  "name": "meteor-react-apollo-accounts",
  "version": "2.1.3",
  "description": "Meteor accounts in GraphQL (Updated)",
  "main": "./client",
  "repository": {
    "url": "https://github.com/appigram/meteor-apollo-accounts",
    "type": "git"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./client",
    "prepublish": "npm run build",
    "test": "npm run lint && jest",
    "watch": "watch npm\\ run\\ build ./src",
    "test-watch": "jest --watch",
    "lint": "standard \"src/**/*.js\""
  },
  "keywords": ["meteor", "apollo", "graphql", "accounts", "react-native"],
  "author": "appigram",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": ">=4.11.0",
    "graphql-tag": "^2.5.0",
    "jest-cli": "^21.2.1",
    "standard": "*"
  },
  "peerDependencies": {
    "graphql-tag": "^2.5.0"
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "/client/"]
  },
  "dependencies": {
    "babel-eslint": "^8.0.2",
    "uglify-js": "^3.1.9",
    "underscore": "^1.8.3"
  },
  "standard": {
    "globals": ["it", "xit", "expect", "test", "jest"]
  }
}
